#!/bin/sh
#
# Insmod
#

auto_mount() {
	e2fsck -fy /dev/mmcblk0p9
	resize2fs /dev/mmcblk0p9
	e2fsck -fy /dev/mmcblk0p8
	resize2fs /dev/mmcblk0p8
	mount -t ext2 /dev/mmcblk0p9 data
	mount -t ext2 /dev/mmcblk0p8 data/cfg
	mount -t  pstore pstore /sys/fs/pstore
}

case "$1" in
  start)
        auto_mount
        ;;
  stop)
        printf "stop finished\n"
        ;;
  *)
        echo "Usage: $0 {start|stop}"
        exit 1
        ;;
esac
exit 0
